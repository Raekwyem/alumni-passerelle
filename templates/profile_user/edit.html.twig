{% extends 'base.html.twig' %}
{% block css %}
    <link rel="stylesheet" href="{{ asset('') }}">
{% endblock %}
{% block body %}
    <h1 class="text-center my-5">Modification du Profil</h1>
    {{ form_start(form) }}

        {{ form_row(form.urlavatar) }}
        {{ form_row(form.urlphoto) }}

        {{ form_row(form.firstname) }}
        {{ form_row(form.lastname) }}

        {{ form_row(form.email) }}
        {{ form_row(form.city) }}

        {{ form_row(form.experiences) }}
        {{ form_row(form.actual) }}
        {{ form_row(form.companyname) }}

        {{ form_row(form.bio) }}
        {{ form_row(form.urllinkedin) }}
        {{ form_row(form.urlportfolio) }}

    {{ form_end(form) }}
    <a href="{{ path('app_adminuser_index') }}" class="btn btn-danger">Annuler</a>
{% endblock %}
